{
  "compilerOptions": {
    "target": "ES2022",
    "module": "Node16",
    "moduleResolution": "Node16",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "outDir": "./dist",
    "rootDir": ".",
    "typeRoots": ["./types", "./node_modules/@types"]
  },
  "include": [
    "./**/*.ts",
    "types/**/*.d.ts"
, "index.ts", "googleAi.js", "gemini-ex.ts"  ],
  "exclude": ["node_modules"]
}
